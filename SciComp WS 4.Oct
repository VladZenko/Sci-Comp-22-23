     "output_type": "execute_result"
    }
   ],
   "source": [
    "h = np.array([2.2, 2.1, 1.8, 2.1, 2.0, 1.9])\n",
    "\n",
    "hbar = np.average(h)\n",
    "hbar\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 161,
   "id": "ec260927",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "0.125646476421096 is the width of surface in meters\n"
     ]
    }
   ],
   "source": [
    "L = len(h)\n",
    "sm_h = 0\n",
    "\n",
    "for i in range(L-1):\n",
    "    \n",
    "    sm_h+=(h[i]-hbar)**2\n",
    "    \n",
    "w = np.sqrt(sm_h/L)\n",
    "print(\"{} is the width of surface in meters\".format(w))\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 164,
   "id": "740791d8",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[2.2] are greater than hbar+w\n",
      "[1.8] are less than hbar-w\n"
     ]
    }
   ],
   "source": [
    "Greater = []\n",
    "Less = []\n",
    "\n",
    "for i in range(len(h)):\n",
    "    \n",
    "    if h[i]>(hbar+w):\n",
    "        Greater.append(h[i])\n",
    "    elif h[i]<(hbar-w):\n",
    "        Less.append(h[i])\n",
    "        \n",
    "print(\"{} are greater than hbar+w\".format(Greater))\n",
    "print(\"{} are less than hbar-w\".format(Less))"
   ]
  },
