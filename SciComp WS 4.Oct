    }
   ],
   "source": [
    "fig = plt.figure()\n",
    "ax = fig.add_subplot(111)\n",
    "ax.plot(E, F[0], label=\"T=1e7\")\n",
    "ax.plot(E, F[1], label=\"T=2e7\")\n",
    "ax.plot(E, F[2], label=\"T=3e7\")\n",
    "ax.set_xlabel(\"E(J)\")\n",
    "ax.set_ylabel(\"F\")\n",
