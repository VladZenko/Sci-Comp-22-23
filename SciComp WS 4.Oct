   "id": "f6e27534",
   "metadata": {},
   "outputs": [],
   "source": [
    "T = np.array([1e7,2e7,3e7])\n",
    "E = np.linspace(0, 1.5e-15, 100)\n",
    "k = 1.38e-23"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 168,
   "id": "e3e35f9b",
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "\n",
    "F = [[],[],[]]\n",
    "\n",
    "for j in range(len(F)):\n",
    "    for i in range(len(E)):\n",
    "\n",
    "        f = (2/np.sqrt(np.pi))*(1/(2*k*T[j]))**(3/2)*(E[i]**0.5)*np.exp(-(E[i]/(k*T[j])))\n",
    "        F[j].append(f)\n"
   ]
  },
  {
